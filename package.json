{
  "name": "@tkomde/iohook",
  "version": "1.1.3",
  "description": "Node.js global keyboard and mouse hook",
  "author": "tkomda",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": {
        "import": "./index.d.mts",
        "require": "./index.d.ts"
      },
      "require": "./index.js",
      "import": "./index.mjs"
    }
  },
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkomde/iohook.git"
  },
  "publishConfig": { "access": "public", "registry": "https://registry.npmjs.org/" },
  "scripts": {
    "install": "node install.js",
    "build": "node build.js --upload=false",
    "build:ci": "node build.js --all --openssl_fips=''",
    "build:print": "node -e 'require(\"./helpers\").printManualBuildParams()'"
  },
  "keywords": [
    "hook",
    "electron",
    "nw.js",
    "listener",
    "mousemove",
    "keypress",
    "keyup",
    "global keypress",
    "shortcut"
  ],
  "lint-staged": {
    "examples/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "docs/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "test/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "nan": "^2.23.0",
    "nugget": "^2.2.0",
    "pump": "^3.0.3",
    "rc": "^1.2.8",
    "tar-fs": "^3.1.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/node": "^24.0.0",
    "electron": "^37.1.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "fs-extra": "^11.3.1",
    "minimist": "^1.2.8",
    "node-abi": "^4.12.0",
    "node-gyp": "^11.4.0",
    "prebuild": "^13.0.1",
    "prettier": "^3.6.2",
    "tar": "^7.4.3"
  },
  "supportedTargets": [
    ["electron", "29.0.0", "121"],
    ["electron", "37.0.0", "136"],
    ["node", "24.0.0", "137"]
  ],
  "iohook": {
    "targets": ["node-137"],
    "platforms": [],
    "arches": ["x64"]
  }
}
